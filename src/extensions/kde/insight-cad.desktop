#!/usr/bin/env xdg-open
[Desktop Entry]
Type=Application
#X-KDE-ServiceTypes=KonqPopupMenu/Plugin
#X-KDE-Submenu=Insight
#MimeType=application/pv-med;application/x-hdf;
MimeType=application/insight-iscad;
Categories=Graphics;VectorGraphics;Engineering;Construction;2DGraphics;Science;
Name=ISCAD editor
Name[de]=ISCAD-Editor
GenericName=Insight CAD Editor
GenericName[de]=Insight CAD Editor
Comment=A script-based CAD editor
Comment[de]=Ein skriptbasierter CAD-Editor
Exec=/bin/bash -lc 'cd `dirname "%f"`; iscad "%f"'
Icon=utilities-terminal
X-KDE-Submenu=ISCAD
